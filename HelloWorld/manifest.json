{
    "manifest_version": 2,
    "name": "PassMeIn",
    "version": "1.0",
    "description": "Pass me in : Making you secure : Developed by mayank, rishabh, souvick & yash",
    
    "icons": {
        "128": "./image/logo.png",
        "46": "./image/logo.png",
        "16": "./image/logo.png"
    },
    "browser_action": {
        "default_icon": "./image/logo.png",
        "default_popup": "popup.html"
    },

    "background":{
        "scripts":["selectusername.js"],
        "persistent":false
    },

    "permissions":[
        "notifications",
        "contextMenus","activeTab"
    ]

}